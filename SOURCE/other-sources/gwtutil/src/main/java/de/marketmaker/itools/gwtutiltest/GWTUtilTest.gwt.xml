<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.5/distro-source/core/src/gwt-module.dtd">
<module>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.i18n.I18N'/>
    <inherits name='de.marketmaker.itools.gwtutil.GWTUtil'/>
    <entry-point class='de.marketmaker.itools.gwtutiltest.client.GwtUtilTestApplication'/>
    <stylesheet src='gwtutiltest.css'/>
    <stylesheet src='gwtutil.css'/>

    <replace-with class="de.marketmaker.itools.gwtutiltest.client.GwtUtilTestIconImageMapping">
        <when-type-is class="de.marketmaker.itools.gwtutil.client.widgets.IconImageMapping"/>
    </replace-with>

<!--
    <set-property name="locale" value="default"/>
    <extend-property name="locale" values="de"/>
-->

    <add-linker name="xsiframe"/>
    <set-configuration-property name="devModeRedirectEnabled" value="true"/>
</module>